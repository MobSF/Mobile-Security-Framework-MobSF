        cable:



          (1) off
          (2) on"
    DEFVAL {on}
::={dvbRcsRcstControl 7}

dvbRcsCtrlDownloadFileCommand OBJECT-TYPE
    SYNTAX              INTEGER {
                                idle            (1),
                                config          (2),
                                installationLog (3)
    }
    MAX-ACCESS          read-write
    STATUS              current
    DESCRIPTION
